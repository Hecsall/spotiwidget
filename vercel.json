{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/"
        },
        {
            "src": "/editor",
            "dest": "/editor"
        },
        {
            "src": "/sign-out",
            "dest": "/sign-out"
        },
        {
            "src": "/widget",
            "dest": "/widget"
        }
    ]
}